              <th>type</th>
              <th>rule</th>
              <th>broker</th>
            </tr>
            <% obj2.forEach(i => {%>
            <tr>
              <form action="home/stocks/<%= i.Symbol %>" method="post">
              <td><%= i.id %></td>
              <td><%= i.type %></td>
              <td><%= i.code%></td>
              <td><%= i.broker%></td> 
